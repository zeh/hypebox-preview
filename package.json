{
  "name": "hypebox-preview",
  "version": "1.0.0",
  "repository": "git@github.com:zeh/hypebox-preview.git",
  "author": "Zeh Fernando <zeh@zehfernando.com>",
  "license": "MIT",
  "scripts": {
    "start": "rimraf dist && parcel serve src/index.html",
    "build": "rimraf dist && cross-env NODE_ENV=production parcel build --no-source-maps src/index.html --public-url .",
    "watch": "rimraf dist && parcel watch src/index.html",
    "lint": "eslint src/ts/**/*.ts src/ts/**/*.tsx",
    "lint:fix": "eslint src/ts/**/*.ts src/ts/**/*.tsx --fix",
    "lint:prettier": "prettier --check src/ts/**/*.ts src/ts/**/*.tsx"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "assemble": "~0.3.11",
    "grunt": "~0.4.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-three-fiber": "^4.0.11",
    "styles": "^0.2.1",
    "three": "^0.112.1",
    "typescript": "^3.7.5",
    "typestyle": "^2.0.4"
  }
}
